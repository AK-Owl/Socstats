            <%= collection_select :country1, @freedom, Freedom.all, :country, :country %>
            <%= collection_select :country2, @freedom, Freedom.all, :country, :country %>
            <%= collection_select :country3, @freedom, Freedom.all, :country, :country %><br><br>
            <%= collection_select :country4, @freedom, Freedom.all, :country, :country %>
            <%= collection_select :country5, @freedom, Freedom.all, :country, :country %>
            <br><br>
          <button type="submit" class="btn">submit</button>
          </div>

        <% end %>

    </div>
</section>
