    <div class="mx-auto text-center">
      <h1 class="mx-auto my-0 text-uppercase">Data Analysis</h1>
    </div>
  </div>
</header>

<section id="analysis" class="analysis-section text-center">
    <div class="container">

          <%= form_tag analysis_results_path, :method => :get do |form| %>
